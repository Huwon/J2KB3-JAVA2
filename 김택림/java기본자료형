package testPjt;

public class MainClass {
	
		public static void main(String[] args) {
			
			char c = 'a';
			System.out.println("c = " + c);
			
			int i = 10;
			System.out.println("i = " + i);
			
			double d = 10.123;
			System.out.println("d = " + d);
			
			boolean b = false;
			System.out.println("b = " + b);
			
			String s = "Hello Java World!!!!";
			System.out.println("s = " + s);
			
			s = "Good";
			System.out.println("s = " + s);
			
		}
}
